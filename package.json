{
	"name": "cninfo",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"eslint": "npx eslint --fix src/",
		"prettier": "npx prettier --write src/",
		"sveltekit": "npx svelte-kit sync && npx paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide && npx svelte-check --tsconfig ./../tsconfig.json --workspace src/",
		"cloc": "npx cloc src/ --not-match-d=\"lib/paraglide\"",
		"i18n": "npx paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20251224.0",
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.36.0",
		"@inlang/paraglide-js": "^2.3.2",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tailwindcss/typography": "^0.5.18",
		"@tailwindcss/vite": "^4.1.13",
		"@types/node": "^22.19.3",
		"drizzle-kit": "^0.31.8",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.39.5",
		"svelte-check": "^4.3.2",
		"sveltekit-rate-limiter": "^0.7.0",
		"sveltekit-sse": "^0.14.3",
		"tailwindcss": "^4.1.13",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.1",
		"vite": "^7.1.7",
		"wrangler": "^4.53.0"
	},
	"dependencies": {
		"@libsql/client": "^0.15.15",
		"cloc": "^2.6.0-cloc",
		"drizzle-orm": "^0.45.1",
		"remixicon": "^4.7.0"
	}
}
